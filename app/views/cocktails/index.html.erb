<div class = "navbar">
  <h1> Cocktails </h1>
  <a class="btn btn-primary btn-sm" href="/cocktails/new" role="button">New Cocktail!</a>
</div>

<div class = "cards">
  <% @cocktails.each do |cocktail| %>
    <div class = "card">
      <%= link_to cocktail.name, cocktail_path(cocktail) %>
    </div>
  <% end %>
</div>
